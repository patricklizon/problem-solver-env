{
  "name": "problem-solving-env",
  "version": "0.0.1",
  "description": "Environment for problem solving with TS in a TDD way",
  "private": true,
  "packageManager": "npm@10.1.0",
  "scripts": {
    "fix:format": "npm run check:format -- --write",
    "fix:lint": "npm run check:lint -- --fix",
    "test:run": "vitest run",
    "test": "vitest",
    "check:format": "prettier --check \"src/**/*.ts\"",
    "check:lint": "eslint",
    "check:types": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "lint:staged": "lint-staged"
  },
  "author": "Patryk Lizo≈Ñ",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-vitest": "^0.3.9",
    "globals": "^13.20.0",
    "prettier": "^2.8.8",
    "typescript": "^5.2.2",
    "vitest": "^0.32.2"
  }
}
